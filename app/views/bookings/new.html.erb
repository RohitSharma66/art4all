<div class="navbar navbar-expand-lg navbar-light navbar-lewagon border-bottom  d-flex justify-content-between">
		<div>
			<%= link_to root_path do  %>
		        <%= image_tag 'logo.png', class: "logo ml-2" %>		
			<% end %>	
		</div>
        <div>
         <%= link_to "Back", arts_path, class: "btn btn-info text-white mr-3"%>
       	 <%= link_to "SignOut", destroy_user_session_path, method: :delete, class: "btn rounded text-white signout-button mr-4"%>
		</div>
</div>

<div class="shadow rounded mt-4 mb-4 col-sm-5" data-aos="flip-left" style="background-color: #fff;">
	<div class="p-4 mt-4">
	<%= simple_form_for [@art, @booking] do |f| %>
	  <%= f.input :start_date, as: :date, html5: true %>
	  <%= f.input :end_date, as: :date, html5: true %>
	   <%= f.input :approved, :as => :hidden, :input_html => { value: false } %>
	  <%= f.submit class: 'btn text-white', style:"background-color:#5bc0be" %>
	<% end %>
</div>